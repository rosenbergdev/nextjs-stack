{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true,
      "modules": true
    }
  },
  "parser": "babel-eslint",
  "rules": {
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }],
    "import/no-unresolved": "off",
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "arrow-parens": 0,
    "comma-dangle": 0,
    "semi": 0,
    "react/jsx-filename-extension": 0
  }
}
